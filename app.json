{
  "name": "CASA",
  "addons": [
    "papertrail:choklad",
    "scheduler:standard",
    {
      "plan": "heroku-postgresql",
      "options": {
        "version": "12"
      }
    }
  ],
  "buildpacks": [
    {
      "url": "heroku/ruby"
    }
  ],
  "formation": {
    "web": {
      "quantity": 1,
      "size": "hobby"
    }
  },
  "scripts": {
    "postdeploy": "bundle exec rake db:schema:load db:seed"
  },
  "stack": "heroku-18"
}
