<div class="row">
  <div class="col-sm-12 dashboard-table-header">
    <h1><%= @learning_hour.name %></h1>
  </div>
</div>

<div class="card card-container">
  <div class="card-body">
    <table class="table table-striped table-bordered learning-hours-list" id="all-learning-hours">
      <thead>
      <tr>
        <th><%= t(".heading.name") %></th>
        <th><%= t(".heading.type") %></th>
        <th><%= t(".heading.occurred_at") %></th>
        <th><%= t(".heading.time") %></th>
        <th><%= t(".heading.action") %></th>
      </tr>
      </thead>
      <tbody>
        <tr>
        <td> <%= @learning_hour.name %> </td>
        <td> <%= @learning_hour.learning_type %> </td>
        <td> <%= @learning_hour.occurred_at.strftime("%B %d, %Y") %> </td>
        <td> <%= @learning_hour.duration_hours %> hr <%= @learning_hour.duration_minutes %> min </td>
        <td> <%= link_to 'edit', edit_volunteer_learning_hour_path %> 
        </td>
        </tr>
      </tbody>
    </table>
  </div>
</div>
